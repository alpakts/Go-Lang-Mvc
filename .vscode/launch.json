    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch MySQL",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "envFile": "${workspaceFolder}/mysql.env",
           "args": ["--env", "mysql.env"]
        },
        {
            "name": "Launch PostgreSQL",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "envFile": "${workspaceFolder}/postgres.env",
           "args": ["--env", "postgres.env"]
        }
    ]
}